                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT Flix.account_firstname as CustomerFirstName, cast(Flix.account_lastname as nvarchar(100)) Collate Latin1_General_CI_AS as CustomerLastName,
cast(Flix.account_email as varchar(200)) as CustomerEmail, cast(Flix.title_id as varchar(20)) as TitleID, null as ProductID, Flix.at_id as AccountTitleID, 
cast(Flix.title_name as varchar(200)) as ItemName, Flix.at_rating as Rating, null as Date
FROM (
SELECT a.account_firstname, cast(a.account_lastname as nvarchar(100)) as account_lastname, a.account_email, t.title_id, t.title_name, act.at_rating, act.at_id
FROM fudgeflix_v3.dbo.ff_accounts a
FULL OUTER JOIN fudgeflix_v3.dbo.ff_account_titles act on act.at_account_id = a.account_id
LEFT JOIN fudgeflix_v3.dbo.ff_titles t on t.title_id = act.at_title_id) as Flix
UNION

select Mart.customer_firstname as CustomerFirstName, cast(Mart.customer_lastname as nvarchar(100)) Collate Latin1_General_CI_AS as CustomerLastName, 
cast(Mart.customer_email as varchar(200)) as CustomerEmail, null as TitleID, cast(Mart.product_id as varchar(20)) as ProductID, null as AccountTitleID, 
cast(Mart.product_name as varchar(200)) as ItemName, Mart.review_stars as Rating, null as Date
from
(SELECT c.customer_firstname, cast(c.customer_lastname as nvarchar(100)) as customer_lastname, c.customer_email, p.product_id,p.product_name, pr.review_stars
FROM fudgemart_v3.dbo.fm_customers c
FULL OUTER join fudgemart_v3.dbo.fm_customer_product_reviews pr on pr.customer_id = c.customer_id
FULL OUTER JOIN fudgemart_v3.dbo.fm_products p on p.product_id = pr.product_id
) as Mart</property>
